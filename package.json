{
  "name": "recast.ly",
  "version": "1.1.0",
  "description": "Making YouTube with React and ES6",
  "author": "Hack Reactor <curriculum.team@hackreactor.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npm run babel\" \"npm run app\" \"npm run test\"",
    "babel": "babel-watch src/index.js",
    "app": "live-server --open=./index.html --port=8082",
    "test": "live-server --open=./SpecRunner.html --port=8081",
    "postinstall": "curl -s https://raw.githubusercontent.com/reactorcore/pomander/master/bin/install | bash"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.2",
    "jquery": "^2.2.1",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "babel-watch": "^2.0.7",
    "chai": "^3.5.0",
    "concurrently": "^3.5.1",
    "mocha": "2.3.3",
    "sinon": "1.17.3"
  }
}
